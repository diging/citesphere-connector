<script
	src="//ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src="/citesphere-review/resources/bootstrap/js/bootstrap.min.js"></script>
<script src="/citesphere-review/resources/bootstrap/js/main.js"></script>
<script src="/citesphere-review/resources/notify/bootstrap-notify.min.js"></script>
<link href="/citesphere-review/resources/notify/animate.css" rel="stylesheet"/>
<script>

$(function() {
	$(".delete-link").click(function(event) {
		var id = $(this).data('group-id');
		$("#delete-client-button").attr("data-group-id", id);
		$("#confirm-client-entry-id").text(id);
		$('#client-delete-confirmation').modal("show");	
		event.preventDefault();
	});
	$("#delete-client-button").click(function() {
		var id = $(this).data("group-id");
		var baseurl = 'auth/group/' + id + '/resync';
		$.ajax({
			'url': baseurl,
			'type': "POST",
			'data': { "_csrf" : "26236e04-3ce5-42db-b4a4-97f1ef09a225" },
			'success': function(data) {
				$.notify('<i class="fas fa-check-circle"></i> Local copy of group has been deleted. Re-sync will start as soon as group is accessed again.', {
					type: 'success',
					offset: {
						x: 50,
						y: 90
					},
					animate: {
						enter: 'animated fadeInRight',
						exit: 'animated fadeOutRight'
					}
				});
			},
			'error': function(data) {
				$.notify('<i class="fas fa-exclamation-circle"></i> Group not found.', {
					type: 'danger',
					offset: {
						x: 50,
						y: 90
					},
					animate: {
						enter: 'animated fadeInRight',
						exit: 'animated fadeOutRight'
					}
				});
			}
		});
		$('#client-delete-confirmation').modal("hide");
		event.preventDefault();
		$(this).removeData();
	});
});


</script>
